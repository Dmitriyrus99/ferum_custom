  "fields": [
    {
      "fieldname": "contract",
      "fieldtype": "Link",
      "label": "Contract",
      "options": "Contract",
      "reqd": 1
    },
    {
      "fieldname": "project",
      "fieldtype": "Link",
      "label": "Project",
      "options": "Project",
      "reqd": 1
    },
    {
      "fieldname": "period_from",
      "fieldtype": "Date",
      "label": "Period From",
      "reqd": 1
