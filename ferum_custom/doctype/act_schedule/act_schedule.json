    {
      "fieldname": "period_to",
      "fieldtype": "Date",
      "label": "Period To",
      "reqd": 1
    },
    {
      "fieldname": "planned_amount",
      "fieldtype": "Currency",
      "label": "Planned Amount",
      "permlevel": 1
